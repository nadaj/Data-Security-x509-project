package main;

import java.security.cert.CertificateFactory;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		int option = 0;
		Scanner in = new Scanner(System.in);
		
		while (true)
		{
			option = in.nextInt();
			switch(option)
			{
				case 0: System.out.println("Odaberite funkcionalnost:");
						break;
				case 1: {
					System.out.println("Generisanje novog para kljuceva");

					try {
						CertificateFactory cf = CertificateFactory.getInstance("X.509");
					
					} catch (Exception e) {
						e.printStackTrace();
					}
				}
			}
		}
	}

}
